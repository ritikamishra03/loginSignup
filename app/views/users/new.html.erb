<h1>Signup</h1>
<%= form_with model:@user, url: signup_path do |form| %>
    <div class="field">
        <%= form.label :email %>
        <%= form.email_field :email %>
    </div>
    
    <br>
    
    <div class="field">
        <%= form.label :password %>
        <%= form.password_field :password %>
    </div>

    <br>

    <div class="action">
        <%= form.submit 'Sign up' %>
    </div>
<%end%>
